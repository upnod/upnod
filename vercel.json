{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/hello.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/hello.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "staticFiles": {
    "public": "public"
  }
}
