{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "scripts.js", "use": "@vercel/static" },
    { "src": "img/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/img/(.*)", "dest": "/img/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
